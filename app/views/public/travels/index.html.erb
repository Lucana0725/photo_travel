<h1>Travels</h1>

<% @travels.each do |travel| %>
  <!--投稿画像-->
  <%= image_tag travel.get_image(300, 300) %>
  
  <!--投稿タイトル-->
  <%= travel.title %>
  
  <!--投稿内容-->
  <%= travel.body %>
<% end %>